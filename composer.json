{
    "name": "patrikgrinsvall/laravel-bankid",
    "description": "Swedish BankId integration for Laravel 8",
    "keywords": [
        "laravel",
        "bankid",
        "bank id",
        "swedish bankid",
        "svenskt bankid",
        "fiansiell id teknik"
    ],
    "homepage": "https://github.com/patrikgrinsvall/laravel-bankid",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Grinsvall",
            "email": "patrik@silentridge.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.21",
        "illuminate/filesystem": "^9.21",
        "illuminate/support": "^9.21",
        "illuminate/validation": "^9.21"

    },
    "require-dev": {
        "spatie/laravel-package-tools": "dev-main",
        "nunomaduro/collision": "^5.3|^v6.3.0",
        "orchestra/testbench": "^6.15|^7.5.0",
        "phpunit/phpunit": "^9.3",
        "spatie/ignition": "^1.4.1",
        "vimeo/psalm": "^4.4"

    },
    "autoload": {
        "psr-4": {
            "Patrikgrinsvall\\LaravelBankid\\": "src",
            "Patrikgrinsvall\\LaravelBankid\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patrikgrinsvall\\LaravelBankid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BankidServiceProvider"
            ],
            "aliases": {
                "LaravelBankid": "Patrikgrinsvall\\LaravelBankid\\BankidFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
